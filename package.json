{
  "name": "nuxt-prepare",
  "type": "module",
  "version": "2.2.0",
  "packageManager": "pnpm@9.14.2",
  "description": "Build actions for Nuxt",
  "author": "Johann Schopplich <hello@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/nuxt-prepare#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/nuxt-prepare.git"
  },
  "bugs": "https://github.com/johannschopplich/nuxt-prepare/issues",
  "keywords": [
    "init",
    "nuxt",
    "prepare",
    "server"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.mjs",
      "require": "./dist/config.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:types": "tsc --noEmit",
    "release": "bumpp"
  },
  "dependencies": {
    "@nuxt/kit": "^3.14.1592",
    "defu": "^6.1.4",
    "importx": "^0.5.0",
    "mlly": "^1.7.3",
    "pathe": "^1.1.2",
    "scule": "^1.3.0",
    "type-fest": "^4.28.1"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.7.2",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.14.1592",
    "@types/node": "^22.10.0",
    "bumpp": "^9.8.1",
    "eslint": "^9.15.0",
    "eslint-plugin-perfectionist": "^4.1.2",
    "nuxt": "^3.14.1592",
    "typescript": "^5.6.3"
  }
}
